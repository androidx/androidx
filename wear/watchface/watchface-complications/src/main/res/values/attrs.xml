<resources>
    <!-- ComplicationSlotBounds may not have any child nodes. -->
    <declare-styleable name="ComplicationSlotBounds">
        <!-- The type of complication. Required if per-complication type bounds are used. -->
        <attr name="complicationType"/>
        <!-- Required. Unit-square complication top bound coordinate. -->
        <attr name="top" format="float" />
        <!-- Required. Unit-square complication bottom bound coordinate. -->
        <attr name="bottom" format="float" />
        <!-- Required. Unit-square complication left bound coordinate. -->
        <attr name="left" format="float" />
        <!-- Required. Unit-square complication right bound coordinate. -->
        <attr name="right" format="float" />
    </declare-styleable>
</resources>
