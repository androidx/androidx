<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/on"
        android:drawable="@drawable/glance_switch_thumb_on"
        android:state_enabled="true" />
    <item
        android:id="@+id/off"
        android:drawable="@drawable/glance_switch_thumb_off" />
    <transition
        android:drawable="@drawable/glance_switch_thumb_off_to_on"
        android:fromId="@+id/off"
        android:toId="@+id/on" />
    <transition
        android:drawable="@drawable/glance_switch_thumb_on_to_off"
        android:fromId="@+id/on"
        android:toId="@+id/off" />
</animated-selector>
